@{
}
<head>

</head>
<body>
<div class="mdl-tabs mdl-js-tabs mdl-js-ripple-effect">
    <div class="mdl-tabs__tab-bar">
        <a href="#simple-panel" class="mdl-tabs__tab is-active">Simple Problems</a>
        <a href="#multiple-panel" class="mdl-tabs__tab">Multiple Triangle Problems</a>
        <a href="#complex-panel" class="mdl-tabs__tab">Complex Problems</a>
    </div>



    @*Simple Problems*@
    <div class="mdl-tabs__panel is-active" id="simple-panel">

        <div class="panel panel-success">

            <div class="mdl-card__title">
                <div class="mdl-card__title-text">Simple Plan View Problems</div>
            </div>

            <div class="mdl-card__supporting-text"><h4>Please enter the respective notation to get the equivalent notations.</h4></div>



            @*radio buttons*@
            <div class="col-sm-12">
                <br />

                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-1">
                    <input type="radio" id="option-1" class="mdl-radio__button" name="options" value="1" onclick="option1()">
                    <span class="mdl-radio__label">only sides</span>
                </label>
                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-2">
                    <input type="radio" id="option-2" class="mdl-radio__button" name="options" value="2" onclick="option2()">
                    <span class="mdl-radio__label">only angles</span>
                </label>
                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-3">
                    <input type="radio" id="option-3" class="mdl-radio__button" name="options" value="3" onclick="option3()">
                    <span class="mdl-radio__label">Finding sides using sides and angles</span>
                </label>
                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-4">
                    <input type="radio" id="option-4" class="mdl-radio__button" name="options" value="4" onclick="option4()">
                    <span class="mdl-radio__label">Finding angles using sides</span>
                </label>
            </div>





            <div class="mdl-card__actions mdl-card--border">


                <form action="#">
                    <br />
                    <br>

                    <div class="container text-center">
                        <div class="col-sm-3">
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant1" name="quadrant1">
                                <label class="mdl-textfield__label" for="quadrant1" id="sidea">Enter a value</label>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant2">
                                <label class="mdl-textfield__label" for="quadrant2" id="sideb">Enter b value</label>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant3">
                                <label class="mdl-textfield__label" for="quadrant3" id="sidec">Enter c value</label>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant4">
                                <label class="mdl-textfield__label" for="quadrant4" id="sideLa">Enter angle A</label>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant6">
                                <label class="mdl-textfield__label" for="quadrant6" id="sideLc">Enter angle C</label>
                            </div>
                        </div>
                        <div class="col-sm-3"></div>
                        <div class="col-sm-3">
                            <div class="mdl-card__title-text">Graphical Orientation </div>

                            <img src="~/images/Triangle.png" />
                        </div>
                    </div>





                    <center>
                        <button type="button" id="btn" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored" onclick="myFunction()">
                            Submit
                        </button>
                    </center>

                </form>
                <br />
                <br />





            </div>


        </div>





    </div>



    @*Multiple Triangle Problems*@
    <div class="mdl-tabs__panel" id="multiple-panel">

        <div class="panel panel-success">

            <form action="#">
                <br />
                <br>

                <div class="container text-center">
                    <div class="col-sm-3">
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-a" name="quadrant1">
                            <label class="mdl-textfield__label" for="value-a">Enter a value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-b">
                            <label class="mdl-textfield__label" for="value-b">Enter b value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-c">
                            <label class="mdl-textfield__label" for="value-c">Enter c value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-d">
                            <label class="mdl-textfield__label" for="value-d">Enter d value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-e">
                            <label class="mdl-textfield__label" for="value-e">Enter e value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-f">
                            <label class="mdl-textfield__label" for="value-f">Enter f value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-LA">
                            <label class="mdl-textfield__label" for="value-LA">Enter angle A</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-LC">
                            <label class="mdl-textfield__label" for="value-LC">Enter angle C</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-LD">
                            <label class="mdl-textfield__label" for="value-LD">Enter angle D</label>
                        </div>

                        <center>
                            <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored" onclick="myFunction2()">
                                Submit
                            </button>
                        </center>
                    </div>
                    
                    <div class="col-sm-6">
                        <img src="~/images/multiple.png" />
                    </div>


                </div>
            </form>





        </div>
    </div>


    @*Complex problems*@
    <div class="mdl-tabs__panel" id="complex-panel">

        <div class="panel panel-success">

            <form action="#">
                <br />
                <br>

                <div class="container text-center">
                    <div class="col-sm-3">
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-a1" name="quadrant1">
                            <label class="mdl-textfield__label" for="value-a1">Enter a value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-b1">
                            <label class="mdl-textfield__label" for="value-b1">Enter b value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-c1">
                            <label class="mdl-textfield__label" for="value-c1">Enter c value</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-d1">
                            <label class="mdl-textfield__label" for="value-d1">Enter d value</label>
                        </div>
                        <br />
                        <br />

                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-LA1">
                            <label class="mdl-textfield__label" for="value-LA1">Enter angle A</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-LB1">
                            <label class="mdl-textfield__label" for="value-LB1">Enter angle B</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-LC1">
                            <label class="mdl-textfield__label" for="value-LC1">Enter angle C</label>
                        </div>
                        <br />
                        <br />
                        <div class="mdl-textfield mdl-js-textfield">
                            <input class="mdl-textfield__input" type="text" id="value-LD1">
                            <label class="mdl-textfield__label" for="value-LD1">Enter angle D</label>
                        </div>


                    </div>

                    <div class="col-sm-2"></div>
                    <div class="col-sm-3">
                        <img src="~/images/Complex.png" />
                    </div>


                </div>
                <center>
                    <button type="button" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored" onclick="myFunction3()">
                        Submit
                    </button>
                </center>
            </form>





        </div>



    </div>

</div>
</body>


















