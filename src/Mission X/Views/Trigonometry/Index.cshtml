@*<meta http-equiv="refresh" content="60" />*@
<head>
    <script type="text/javascript">
        var img;
        function openWin(img)
        {
          
           window.open(img,"mywin","menubar=0,resizable=0,width=400,height=300");
        }
        function openWin1(img) {

            window.open(img, "mywin", "menubar=0,resizable=0,width=400,height=300");
        }
        function openWin2(img) {

            window.open(img, "mywin", "menubar=0,resizable=0,width=400,height=300");
        }
    </script>

</head>
<body>
<div class="mdl-tabs mdl-js-tabs mdl-js-ripple-effect">
    <div class="mdl-tabs__tab-bar">
        <a href="#simple-panel" class="mdl-tabs__tab is-active">Simple Problems</a>
        <a href="#multiple-panel" class="mdl-tabs__tab">Multiple Triangle Problems</a>
        <a href="#complex-panel" class="mdl-tabs__tab">Complex Problems</a>
    </div>



    @*Simple Problems*@
    <div class="mdl-tabs__panel is-active" id="simple-panel">

        <div class="panel panel-success">

            <div class="mdl-card__title">
                <div class="mdl-card__title-text">Simple Plan View Problems</div>
            </div>

            <div class="mdl-card__supporting-text"><h4>Please enter the respective notation to get the equivalent notations.</h4></div>



            @*radio buttons*@
            <div class="col-sm-10">
                <br />

                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-1">
                    <input type="radio" id="option-1" class="mdl-radio__button" name="options" value="1" onclick="option1()">
                    <span class="mdl-radio__label">only sides</span>
                </label>
                &nbsp;
                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-2">
                    <input type="radio" id="option-2" class="mdl-radio__button" name="options" value="2" onclick="option2()">
                    <span class="mdl-radio__label">only angles</span>
                </label>
                &nbsp;
                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-3">
                    <input type="radio" id="option-3" class="mdl-radio__button" name="options" value="3" onclick="option3()">
                    <span class="mdl-radio__label">Finding sides using sides and angles</span>
                </label>
                &nbsp;
                <label class="mdl-radio mdl-js-radio mdl-js-ripple-effect" for="option-4">
                    <input type="radio" id="option-4" class="mdl-radio__button" name="options" value="4" onclick="option4()">
                    <span class="mdl-radio__label">Finding angles using sides</span>
                </label>
                
            </div>
            <div class="col-sm-2">
                <br/>
               

                
                <button id="help" class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-button--colored" onclick="openWin('/images/Triangle.png')" >
                    <i class="material-icons">?</i>
                </button>
                
                <div class="mdl-tooltip" data-mdl-for="help">
                    Reference Diagram
                </div>
             </div>





            <div class="mdl-card__actions mdl-card--border">


                <form action="#">
                    <br />
                    <br>

                    <div class="container text-center">
                        <div class="col-sm-3">
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant1" oninput="ValidToA(this.value)">
                                <label class="mdl-textfield__label" for="quadrant1" id="sidea" onchange="">Enter a value</label>
                                <p id="Sa" style="color : red; font : bold 18px arial,verdana;"></p>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant2" oninput="ValidToB(this.value)">
                                <label class="mdl-textfield__label" for="quadrant2" id="sideb">Enter b value</label>
                                <p id="Sb" style="color : red; font : bold 18px arial,verdana;"></p>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant3" oninput="ValidToC(this.value)">
                                <label class="mdl-textfield__label" for="quadrant3" id="sidec">Enter c value</label>
                                <p id="Sc" style="color : red; font : bold 18px arial,verdana;"></p>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant4" oninput="ValidToLA(this.value)">
                                <label class="mdl-textfield__label" for="quadrant4" id="sideLa">Enter angle A</label>
                                <p id="SLa" style="color : red; font : bold 18px arial,verdana;"></p>
                            </div>
                            <br />
                            <br />
                            <div class="mdl-textfield mdl-js-textfield">
                                <input class="mdl-textfield__input" type="text" id="quadrant6" oninput="ValidToLC(this.value)">
                                <label class="mdl-textfield__label" for="quadrant6" id="sideLc">Enter angle C</label>
                                <p id="SLc" style="color : red; font : bold 18px arial,verdana;"></p>
                            </div>
                        </div>
                        <br/>
                        <br />
                        <div class="col-sm-3">
                           <b>units   : </b>
                            <select id="Distance">
                                
                                <option value="m" id="Meters">Meters</option>
                                <option value="mi" id="Miles">Miles</option>
                                <option value="km" id="Kilometres">Kilometers</option>
                                <option value="cm" id="Centi">Centimeters</option>
                            </select>
                        </div>
                            <div class="col-sm-5">

                                <div class="panel panel-success">
                                    
                                    <div class="panel-heading">Graphical Orientation for Inputs Produced</div>
                                    <div class="panel-body"><center> 
                                        <canvas width="400" height="400" id="Simple_Canvas">
                                                                         
                                        </canvas></center>
                                        <a id="download" onclick="down()">Download as image</a>
                                    </div>
                                </div>

                            </div>
                        <div class="col-sm-1">
                            </div>
                        </div>




                    <div class="container text-center">
                        <div class="col-sm-6">
                            <center>
                                <button type="button" id="btn" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored" onclick="myFunction()">
                                    Submit
                                </button>
                                
                            </center>
                        </div>

                        <div class="col-sm-6">
                            <center>
                                <button type="button" id="reload" class="mdl-button mdl-js-button mdl-button--raised mdl-js-ripple-effect mdl-button--colored" onclick="refresh()">
                                    reload
                                </button>
                            </center>
                        </div>
                        </div>
</form>
                <br />
                <br />





            </div>


        </div>





    </div>



