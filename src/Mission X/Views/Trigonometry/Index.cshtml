                        </select>
                    </div>
                    
                        <div class="col-sm-6">
                            <div class="panel panel-success">

                                <div class="panel-heading">Graphical Orientation for Inputs Produced</div>
                                <div class="panel-body">
                                    <center>
                                        <canvas width="330" height="400" id="my_Canvas2"></canvas>
                                        <a id="download" onclick="down()">Download as image</a>
                                    </center>

                                </div>
                            </div>
                        </div>
                        @*<div class="col-sm-2">
                            <button id="help1" class="mdl-button mdl-js-button mdl-button--fab mdl-button--mini-fab mdl-button--colored" onclick="openWin1('/images/multiple.png')">
